{
  "name": "cypress-intermediario-v2",
  "version": "1.0.0",
  "description": "Repositório do projeto v2 do curso de testes automatizados com Cypress (intermdiário) da Escola TAT no Udemy",
  "main": "cypress.config.js",
  "engines": {
    "node": "20.12.0"
  },
  "scripts": {
    "cypress:web": "npx cypress open",
    "cypress:mobile": "npx cypress open --config viewportWidth=410 viewportHeight=860",
    "cypress:headless": "npx cypress run --browser electron",
    "cypress:headless:mobile": "npx cypress run --browser electron --config viewportWidth=410 viewportHeight=860",
    "lint": "eslint \"./**/*.{ts,tsx,js,jsx,css,html}\" --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint \"./**/*.{ts,tsx,js,jsx,css,html}\" --fix",
    "format": "prettier \"./**/*.{ts,tsx,js,jsx,css,html}\" --write",
    "check": "prettier \"./**/*.{ts,tsx,js,jsx,css,html}\" --check"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wlsf82/cypress-intermediario-v2.git"
  },
  "keywords": [
    "Testing",
    "Cypress.io",
    "Cypress intermediário",
    "Talking About Testing",
    "E2E Testing"
  ],
  "author": "Walmyr Filho <wlsf82@gmail.com> (https://walmyr.dev/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wlsf82/cypress-intermediario-v2/issues"
  },
  "homepage": "https://github.com/wlsf82/cypress-intermediario-v2#readme",
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@rocketseat/eslint-config": "^2.2.2",
    "cypress": "^12.0.2",
    "cypress-plugin-api": "^2.6.1",
    "eslint": "^8.57.0"
  }
}